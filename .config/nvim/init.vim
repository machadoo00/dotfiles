"Carga de archivo con plugins y mapeo de teclas"
so ~/.config/nvim/plugins.vim " Plugins link
so ~/.config/nvim/maps.vim " Mapeo Links

"Settings"
set number " Numero de lineas
set expandtab
set modifiable
set encoding=UTF-8 " Codificaci√≥n
set relativenumber " Numero relativo
set showcmd
set hidden " Guardar relax

let g:gruvbox_contrast_dark = 'hard'
colorscheme gruvbox
